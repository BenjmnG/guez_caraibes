---
permalink: /sitemap.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{{ collections.all |debug  }}
    {% for page in collections.all %}
        {# Remove deadend pages #}
        {% if 
            not (page.inputPath | startsWith('./contenu/projets/'))
            and not (page.inputPath | startsWith('./contenu/savoir-faire/'))
            and not (page.inputPath | startsWith('./contenu/equipes/'))
         %}
            <url>
                <loc>{{ website.url }}{{ page.url | url }}</loc>
                <lastmod>{{ page.date.toISOString() }}</lastmod>
            </url>
        {% endif %}
    {% endfor %}
</urlset>